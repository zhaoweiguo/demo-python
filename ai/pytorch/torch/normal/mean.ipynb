    "import torch\n",
    "\n",
    "# 创建一个张量\n",
    "tensor = torch.tensor([[1.0, 2.0, 3.0], [3.0, 4.0, 5.0]])\n",
    "print(tensor.shape)\n",
    "\n",
    "# 计算张量的平均值\n",
    "mean_value = torch.mean(tensor)\n",
    "\n",
    "# 打印结果\n",
    "print(\"张量的平均值：\", torch.mean(tensor))\n",
    "print(\"张量的平均值：\", torch.mean(tensor, dim=0))\n",
    "print(\"张量的平均值：\", torch.mean(tensor, dim=1))"
